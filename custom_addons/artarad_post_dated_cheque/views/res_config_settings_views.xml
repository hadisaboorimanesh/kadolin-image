<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_finnotech" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.finnotech</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//app[@name='account']" position="inside">
                <div class="app_settings_block" data-string="Finnotech API" string="Finnotech API">
                    <block title="Finnotech Web Service Settings">

                        <setting help="">
                            <field name="finnotech_client_id" string="Finnotech Client Id"/>
                        </setting>
                        <setting help="">
                            <field name="national_id" string="National ID" />
                        </setting>

                    </block>


                </div>

            </xpath>
        </field>
    </record>
</odoo>
