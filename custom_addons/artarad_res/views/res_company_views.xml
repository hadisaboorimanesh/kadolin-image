<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_form_artarad_res_company" model="ir.ui.view">
        <field name="name">view.form.artarad.res.company</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="numbers_validity_check" />
                <field name="numbers_uniqueness_check" />
            </xpath>
        </field>
    </record>

</odoo>